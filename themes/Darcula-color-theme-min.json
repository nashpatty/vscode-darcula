            "settings": {
                "foreground": "#C59172"
            }
        },
        {
            "scope": "storage.type",
            "settings": {
                "foreground": "#C59172"
            }
        },
        {
            "scope": [
                "storage.modifier",
                "keyword.operator.noexcept"
