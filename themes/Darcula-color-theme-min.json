    "tokenColors": [
        {
            "name": "keyword or other syntax",
            "scope": "punctuation.definition.variable,punctuation.terminator,constant.character.escape",
            "settings": {
                "foreground": "#C59172"
            }
        },
        {
            "name": "no highlight",
            "scope": "variable.parameter,variable.other,string.unquoted.argument.shell,storage.type.object,storage.type.java,constant.other.option",
            "settings": {
                "foreground": "#BCBEC3"
            }
        },
        {
            "scope": [
                "meta.embedded",
                "source.groovy.embedded",
                "string meta.image.inline.markdown",
                "variable.legacy.builtin.python"
            ],
            "settings": {
                "foreground": "#BCBEC3"
            }
        },
        {
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "header",
            "settings": {
                "foreground": "#000080"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#7a7e85"
            }
        },
        {
            "name": "block comment",
            "scope": "comment.block",
            "settings": {
                "foreground": "#5f826b",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#C59172"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "variable.other.enummember",
                "keyword.operator.plus.exponent",
                "keyword.operator.minus.exponent"
            ],
            "settings": {
                "foreground": "#2aacb8"
            }
        },
        {
            "scope": "constant.regexp",
            "settings": {
                "foreground": "#646695"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#C59172"
            }
        },
        {
            "scope": [
                "entity.name.tag.css",
                "entity.name.tag.less"
            ],
            "settings": {
                "foreground": "#d7ba7d"
            }
        },
        {
